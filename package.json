{
  "name": "async-retry",
  "version": "0.2.1",
  "description": "Retrying made simple, easy and async",
  "main": "./lib/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "xo && ava"
  },
  "greenkeeper": {
