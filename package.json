    "space": true,
    "semicolon": false,
    "ignores": [
      "examples/**/*"
    ]
  },
  "dependencies": {
    "retry": "0.10.0",
    "babel-runtime": "6.11.6"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "node-fetch": "1.6.3",
    "then-sleep": "1.0.1",
    "xo": "0.17.0"
  }
}
